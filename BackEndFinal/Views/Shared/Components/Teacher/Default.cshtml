@model Task<List<Teacher>>
@foreach (var item in await Model)
{
    <div class="col-md-3 col-sm-4 col-xs-12">
        <div class="single-teacher mb-45">
            <div class="single-teacher-img">
                <a href="teacher-details.html"><img src="img/teacher/teacher1.jpg" alt="teacher"></a>
            </div>
            <div class="single-teacher-content text-center">
                <h2><a href="teacher-details.html">@item.Name</a></h2>
                <h4>@item.Position</h4>
                <h4>@(item.courseTeachers?.FirstOrDefault()?.Course?.Title ?? "There is no course assigned to the teacher")</h4>
                <ul>
                    <li><a href="@item.TeacherContactInfo?.FaceBookUrl"><i class="zmdi zmdi-facebook"></i></a></li>
                    <li><a href="@item.TeacherContactInfo?.IntaUrl"><i class="zmdi zmdi-pinterest"></i></a></li>
                    <li><a href="@item.TeacherContactInfo?.pinterestUrl"><i class="zmdi zmdi-vimeo"></i></a></li>
                    <li><a href="@item.TeacherContactInfo?.SkypeUrl"><i class="zmdi zmdi-twitter"></i></a></li>
                </ul>
            </div>
        </div>
    </div>
}